import type { CollectionConfig } from 'payload'

export const {{pascalCase name}}: CollectionConfig = {
  slug: '{{lowerCase name}}',
  admin: {
    useAsTitle: 'title',
  },
  access: {
    read: () => true,
  },
  fields: [
    {
      name: 'title',
      type: 'text',
    }
  ]
}
